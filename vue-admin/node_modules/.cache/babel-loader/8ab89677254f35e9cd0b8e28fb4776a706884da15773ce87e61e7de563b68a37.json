style=\"width: 500px;height: 400px\">这是主页</div>\r\n                </el-col>\r\n            </el-card>\r\n        </el-row>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport * as echarts from \"echarts\"\r\n\r\nexport default {\r\n    name: \"Index\",\r\n    data() {\r\n        return {\r\n            total: 0,\r\n            articleTotal: 0,\r\n            onlineTotal: 0,\r\n            yiyan: '',\r\n            author: '',\r\n            loading: false\r\n        }\r\n    },\r\n    mounted() {\r\n        var chartDom = document.getElementById('main');\r\n        var myChart = echarts.init(chartDom);\r\n        var option;\r\n\r\n        option = {\r\n            title: {\r\n                text: '各季度注册人数统计',\r\n                subtext: '趋势图',\r\n                left: 'left'\r\n            },\r\n            tooltip: {\r\n                trigger: 'item'\r\n            },\r\n            legend: {\r\n                orient: 'vertical',\r\n                left: 'left',\r\n                data: ['第一季度', '第二季度', '第三季度', '第四季度']\r\n            },\r\n            toolbox: {\r\n                size: 'mini',\r\n                show: true,\r\n                feature: {\r\n                    dataView: {show: true, readOnly: false},\r\n                    magicType: {show: true, type: ['line', 'bar']},\r\n                    restore: {show: true},\r\n                    saveAsImage: {show: true}\r\n                }\r\n            },\r\n            calculable: true,\r\n            xAxis: {\r\n                type: 'category',\r\n                data: ['第一季度', '第二季度', '第三季度', '第四季度']\r\n            },\r\n            yAxis: {\r\n                type: 'value',\r\n            },\r\n            series: [\r\n                {\r\n                    data: [],\r\n                    type: 'line',\r\n                    label: 'name'\r\n                },\r\n                {\r\n                    data: [],\r\n                    type: 'bar',\r\n                }\r\n            ]\r\n        };\r\n        var piecChartDom = document.getElementById('pie');\r\n        var pieMyChart = echarts.init(piecChartDom);\r\n        var pieOption;\r\n        pieOption = {\r\n            title: {\r\n                text: '各季度注册人数统计',\r\n                subtext: '比例图',\r\n                left: 'center'\r\n            },\r\n            tooltip: {\r\n                trigger: 'item'\r\n            },\r\n            legend: {\r\n                orient: 'vertical',\r\n                left: 'left'\r\n            },\r\n            series: [\r\n                {\r\n                    name: '人数',\r\n                    type: 'pie',\r\n                    radius: '50%',\r\n                    data: [],\r\n                    emphasis: {\r\n                        itemStyle: {\r\n                            shadowBlur: 10,\r\n                            shadowOffsetX: 0,\r\n                            shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n                        }\r\n                    }\r\n                }\r\n            ]\r\n        };\r\n        this.request.get(\"/echarts/mebers\").then(res => {\r\n            this.total = res.data.total\r\n            this.articleTotal = res.data.articleTotal\r\n            this.onlineTotal = res.data.onlineTotal\r\n            // option.xAxis.data = res.data.y;\r\n            option.series[0].data = res.data.records;\r\n            option.series[1].data = res.data.records;\r\n            myChart.setOption(option);\r\n\r\n            pieOption.series[0].data = [\r\n                {value: res.data.records[0], name: '第一季度'},\r\n                {value: res.data.records[1], name: '第二季度'},\r\n                {value: res.data.records[2], name: '第三季度'},\r\n                {value: res.data.records[3], name: '第四季度'},\r\n            ]\r\n            pieMyChart.setOption(pieOption);\r\n        })\r\n        this.yy();\r\n    },\r\n    methods: {\r\n        yy() {\r\n            this.loading = true\r\n            setTimeout(() => {\r\n                this.request.get(\"https://v1.hitokoto.cn/?c=f&encode=josn\").then(res => {\r\n                    this.yiyan = res.hitokoto;\r\n                    this.author = res.creator;\r\n                    this.loading = false;\r\n                })\r\n            }, 2000);\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n#app{\r\n    line-height: 2px;\r\n}\r\nul {\r\n  padding: 0;\r\n  margin: 0;\r\n}\r\n\r\nul {\r\n  list-style-type: none;\r\n  margin-top: 10px;\r\n}\r\n\r\nh4 {\r\n  margin-top: 0;\r\n}\r\n</style>"],"mappings":"AA6IA,YAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,YAAA;MACAC,WAAA;MACAC,KAAA;MACAC,MAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,IAAAC,QAAA,GAAAC,QAAA,CAAAC,cAAA;IACA,IAAAC,OAAA,GAAAb,OAAA,CAAAc,IAAA,CAAAJ,QAAA;IACA,IAAAK,MAAA;IAEAA,MAAA;MACAC,KAAA;QACAC,IAAA;QACAC,OAAA;QACAC,IAAA;MACA;MACAC,OAAA;QACAC,OAAA;MACA;MACAC,MAAA;QACAC,MAAA;QACAJ,IAAA;QACAjB,IAAA;MACA;MACAsB,OAAA;QACAC,IAAA;QACAC,IAAA;QACAC,OAAA;UACAC,QAAA;YAAAF,IAAA;YAAAG,QAAA;UAAA;UACAC,SAAA;YAAAJ,IAAA;YAAAK,IAAA;UAAA;UACAC,OAAA;YAAAN,IAAA;UAAA;UACAO,WAAA;YAAAP,IAAA;UAAA;QACA;MACA;MACAQ,UAAA;MACAC,KAAA;QACAJ,IAAA;QACA7B,IAAA;MACA;MACAkC,KAAA;QACAL,IAAA;MACA;MACAM,MAAA,GACA;QACAnC,IAAA;QACA6B,IAAA;QACAO,KAAA;MACA,GACA;QACApC,IAAA;QACA6B,IAAA;MACA;IAEA;IACA,IAAAQ,YAAA,GAAA5B,QAAA,CAAAC,cAAA;IACA,IAAA4B,UAAA,GAAAxC,OAAA,CAAAc,IAAA,CAAAyB,YAAA;IACA,IAAAE,SAAA;IACAA,SAAA;MACAzB,KAAA;QACAC,IAAA;QACAC,OAAA;QACAC,IAAA;MACA;MACAC,OAAA;QACAC,OAAA;MACA;MACAC,MAAA;QACAC,MAAA;QACAJ,IAAA;MACA;MACAkB,MAAA,GACA;QACApC,IAAA;QACA8B,IAAA;QACAW,MAAA;QACAxC,IAAA;QACAyC,QAAA;UACAC,SAAA;YACAC,UAAA;YACAC,aAAA;YACAC,WAAA;UACA;QACA;MACA;IAEA;IACA,KAAAC,OAAA,CAAAC,GAAA,oBAAAC,IAAA,CAAAC,GAAA;MACA,KAAAhD,KAAA,GAAAgD,GAAA,CAAAjD,IAAA,CAAAC,KAAA;MACA,KAAAC,YAAA,GAAA+C,GAAA,CAAAjD,IAAA,CAAAE,YAAA;MACA,KAAAC,WAAA,GAAA8C,GAAA,CAAAjD,IAAA,CAAAG,WAAA;MACA;MACAU,MAAA,CAAAsB,MAAA,IAAAnC,IAAA,GAAAiD,GAAA,CAAAjD,IAAA,CAAAkD,OAAA;MACArC,MAAA,CAAAsB,MAAA,IAAAnC,IAAA,GAAAiD,GA